<idea-plugin>
    <id>org.example.quebee_v2</id>
    <name>quebee_v2</name>
    <vendor email="test@test.com" url="http://test.com">test</vendor>

    <description>description of my super puper plugin description of my super puper plugin</description>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->

    <depends>com.intellij.modules.database</depends>
    <depends>com.intellij.modules.ultimate</depends>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity implementation="org.quebee.com.DebugGUI"/>
    </extensions>

    <actions>
        <action id="com.querybuilder.MainAction"
                class="org.quebee.com.MainAction"
                text="Open SQL QueBee to Edit Query"
                description="Open SQL QueBee to edit query">
<!--                icon="/images/qbicon.png">-->
            <add-to-group group-id="Console.Jdbc.Left" anchor="after" relative-to-action="Database.Settings"/>
        </action>
    </actions>

    <projectListeners>
        <listener class="org.quebee.com.DbPsiFacadeListener"
                  topic="com.intellij.database.psi.DbPsiFacade$Listener"/>
    </projectListeners>
    <projectListeners>
        <listener class="org.quebee.com.DataSourceStorageListener"
                  topic="com.intellij.database.dataSource.DataSourceStorage$Listener"/>
    </projectListeners>
</idea-plugin>